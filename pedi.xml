	<weight>
		<setup title="Weight">
			<drug id="Adenosine">
				<quantity>mg</quantity>
				<dose>0.1-0.2mg/kg</dose>
				<remark>IV, IO (rapid). May repeat in 2 minutes. Maximum single dose: 12mg. Follow with saline flush.</remark>
			</drug>
			<drug id="Amiodarone">
				<quantity>mg</quantity>
				<dose>5mg/kg</dose>
				<remark>IV, IO. Bolus for pulseless arrest. Infuse over 20-60 minimum for tachyarrythmias. Total maximum 15mg/kg.</remark>
			</drug>
			<drug id="Atropine">
				<quantity>mg</quantity>
				<dose>0.02mg/kg</dose>
				<remark>IV, IO, ET. Minimum dose: 0.1mg. Maximum single dose: 0.5mg in child. May repeat twice.</remark>
			</drug>
			<drug id="Versed">
				<quantity>mg</quantity>
				<dose>0.1mg/kg</dose>
				<remark>IV, IO (slowly titrate to seizure activity/sedation). May use up to 0.5mg/kg if given rectally.</remark>
			</drug>
			<drug id="Epi 1:10,000">
				<quantity>mg</quantity>
				<dose>0.01mg/kg</dose>
				<remark>For pulseless arrest. IV, IO. Use 0.1mg/kg of 1:1,000 solution for ET doses. Repeat q 3-5 minutes.</remark>
			</drug>
			<drug id="Lidocaine">
				<quantity>mg</quantity>
				<dose>1mg/kg</dose>
				<remark>IV, IO, ET. May repeat q 5-10 minutes. Maximum total dose: 3mg/kg</remark>
			</drug>
			<drug id="Narcan">
				<quantity>mg</quantity>
				<dose>0.1mg/kg</dose>
				<remark>IV, IO, ET, IM, SQ, Nasal. Maximum single dose: 2mg</remark>
			</drug>
		</setup>
